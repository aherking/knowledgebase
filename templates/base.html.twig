<!-- TOP NAVIGATION -->
<div class="container-fluid">
    <div class="navigation">
        <div class="row">
            <ul class="topnav">
                {% block topnavigation %}
                    <div class="dropdown">
                        <button type="button" class="btn btn-primary dropdown-toggle" data-toggle="dropdown">
                            Dropdown button
                        </button>
                        <div class="dropdown-menu">
                            <a class="dropdown-item" href="#">Link 1</a>
                            <a class="dropdown-item" href="#">Link 2</a>
                            <a class="dropdown-item" href="#">Link 3</a>
                        </div>
                    </div>
                {% endblock %}
            </ul>
        </div>
    </div>
</div>
<!-- END TOP NAVIGATION -->
{% block searchbar %}
<!-- SEARCH FIELD AREA -->
<div class="searchfield bg-hed-six">
    <div class="container" style="padding-top: 20px; padding-bottom: 20px;">
        <div class="row text-center margin-bottom-20">
            <h1 class="white"> Shopware Knowledge Base</h1>
            <span class="nested"> your place to learn </span>
        </div>
        <br>
        <div class="row search-row">
            <div class="col-md-12">
                <input type="text" class="search" placeholder="Suchbegriff eingeben"> <a class="buttonsearch btn btn-info btn-lg" href="search-results.html">Search</a>
            </div>
        </div>
    </div>
</div>
<!-- END SEARCH FIELD AREA -->
{% endblock %}
<!-- MAIN SECTION -->
<div class="container featured-area-dark padding-30">
    <!-- ARTICLE CATEGORIES SECTION -->
    <div class="row">
        <div class="col-lg-12">
            {% block bodyheadingstyle %}
            <div class="fb-heading" align ="center">{% block bodyheader %}Artikel Tags{% endblock %}</div>
            <hr class="style-two">
            {% endblock %}
            <div class="row">

                {% block body %}

            {% endblock %}
        </div>
    </div>
</div>
<!-- END ARTICLES CATEOGIRES SECTION -->

</div>
</div>
<!-- END MAIN SECTION -->

<!-- END FOOTER -->
<!-- COPYRIGHT INFO -->
<div class="container-fluid footer-copyright marg30">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="float-left">Copyright &#xA9; 2019 #hacktime by TE</div>
                <div class="float-right">
                    <i class="fa fa-facebook"></i> &#xA0;
                    <i class="fa fa-twitter"></i> &#xA0;
                    <i class="fa fa-linkedin"></i>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- END COPYRIGHT INFO -->
<!-- LOADING MAIN JAVASCRIPT -->
{% block javascripts %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
    <script src="{{ asset('build/app.js') }}"></script>
{% endblock %}

</body>

</html>